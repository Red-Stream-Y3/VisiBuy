{
  "expo": {
    "name": "VisiBuy",
    "slug": "VisiBuy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "VisiBuy needs access to your Camera.",
        "NSMicrophoneUsageDescription": "VisiBuy needs access to your Microphone."
      }
    },
    "android": {
      "permissions": [
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.RedStream.VisiBuy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "photosPermission": "Allow Visibuy Admin to access your photo",
          "cameraPermission": "Allow Visibuy Admin to access your camera"
        }
      ]
    ]
  }
}
